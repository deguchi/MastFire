{
  "manifest_version": 2,
  "name": "MastoFire",
  "short_name": "MastoFire",
  "description": "MML Player on Mastodon",
  "version": "1.0.3",
  "icons": {
    "16": "tsscc16.png",
    "48": "tsscc48.png",
    "128": "tsscc128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*", "file://*"],
      "js": ["tsscc.min.js"]
    }
  ]
}
